{
  "version": "1.0",
  "defaultProvider": "cdnjs",
	"libraries": [
		{
			"library": "twitter-bootstrap@5.1.3",
			"destination": "wwwroot/lib/twitter-bootstrap/"
		},
		{
			"library": "jquery.nicescroll@3.7.6",
			"destination": "wwwroot/lib/jquery.nicescroll/",
			"files": [
				"jquery.nicescroll.min.js"
			]
		},
		{
			"library": "hover.css@2.3.1",
			"destination": "wwwroot/lib/hover.css/",
			"files": [
				"css/hover-min.css"
			]
		},
		{
			"library": "jquery@3.6.0",
			"destination": "wwwroot/lib/jquery/",
			"files": [
				"jquery.min.js",
				"jquery.slim.min.js",
				"jquery.slim.min.map",
				"jquery.min.map"
			]
		},
		{
			"library": "jquery-validation-unobtrusive@3.2.12",
			"destination": "wwwroot/lib/jquery-validation-unobtrusive/",
			"files": [
				"jquery.validate.unobtrusive.min.js"
			]
		},
		{
			"library": "jquery-validate@1.19.3",
			"destination": "wwwroot/lib/jquery-validate/",
			"files": [
				"jquery.validate.min.js"
			]
		},
		{
			"provider": "cdnjs",
			"library": "font-awesome@6.1.1",
			"destination": "wwwroot/lib/font-awesome/",
			"files": [
				"css/all.min.css",
				"js/all.min.js",
				"webfonts/fa-brands-400.ttf",
				"webfonts/fa-brands-400.woff2",
				"webfonts/fa-regular-400.ttf",
				"webfonts/fa-regular-400.woff2",
				"webfonts/fa-solid-900.ttf",
				"webfonts/fa-solid-900.woff2",
				"webfonts/fa-v4compatibility.ttf",
				"webfonts/fa-v4compatibility.woff2"
			]
		}
	]
}